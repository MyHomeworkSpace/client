.datePickerCalendarGrid
	padding-left: 2px
	.datePickerCalendarGridItem
		display: inline-block
		text-align: center
		width: 35px
	
	.datePickerCalendarGridItemDay
		border-radius: 5px
		font-size: 14px
		cursor: pointer
		height: 30px
		line-height: 30px
	
	.datePickerCalendarGridItemDay:hover
		background: #eee
		
	.datePickerCalendarGridItemDay.datePickerCalendarGridItemDaySelected
		background: #ccc